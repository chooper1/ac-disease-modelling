	pages = {509--516},
	file = {Full Text:C\:\\Users\\duane\\Zotero\\storage\\EK37RIE3\\Wallinga and Teunis - 2004 - Different epidemic curves for severe acute respira.pdf:application/pdf}
}

@article{cauchemez_real-time_2006,
	title = {Real-time estimates in early detection of {SARS}},
	volume = {12},
	issn = {1080-6040},
	doi = {10.3201/eid1201.050593},
	abstract = {We propose a Bayesian statistical framework for estimating the reproduction number R early in an epidemic. This method allows for the yet-unrecorded secondary cases if the estimate is obtained before the epidemic has ended. We applied our approach to the severe acute respiratory syndrome (SARS) epidemic that started in February 2003 in Hong Kong. Temporal patterns of R estimated after 5, 10, and 20 days were similar. Ninety-five percent credible intervals narrowed when more data were available but stabilized after 10 days. Using simulation studies of SARS-like outbreaks, we have shown that the method may be used for early monitoring of the effect of control measures.},
