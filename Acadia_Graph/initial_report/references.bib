	number = {5},
	urldate = {2020-04-29},
	journal = {American Journal of Preventive Medicine},
	author = {Lee, Bruce Y. and Brown, Shawn T. and Cooley, Philip and Grefenstette, John J. and Zimmerman, Richard K. and Zimmer, Shanta M. and Potter, Margaret A. and Rosenfeld, Roni and Wheaton, William D. and Wiringa, Ann E. and Bacon, Kristina M. and Burke, Donald S.},
	month = nov,
	year = {2010},
	pages = {e21--e29},
	file = {ScienceDirect Snapshot:C\:\\Users\\duane\\Zotero\\storage\\3BRL6KCZ\\S0749379710004435.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\duane\\Zotero\\storage\\PP6MPGS6\\Lee et al. - 2010 - Vaccination Deep Into a Pandemic Wave Potential M.pdf:application/pdf}
}

@article{el-sayed_social_2012,
	title = {Social network analysis and agent-based modeling in social epidemiology},
	volume = {9},
	issn = {1742-5573},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3395878/},
	doi = {10.1186/1742-5573-9-1},
	abstract = {The past five years have seen a growth in the interest in systems approaches in epidemiologic research. These approaches may be particularly appropriate for social epidemiology. Social network analysis and agent-based models (ABMs) are two approaches that have been used in the epidemiologic literature. Social network analysis involves the characterization of social networks to yield inference about how network structures may influence risk exposures among those in the network. ABMs can promote population-level inference from explicitly programmed, micro-level rules in simulated populations over time and space. In this paper, we discuss the implementation of these models in social epidemiologic research, highlighting the strengths and weaknesses of each approach. Network analysis may be ideal for understanding social contagion, as well as the influences of social interaction on population health. However, network analysis requires network data, which may sacrifice generalizability, and causal inference from current network analytic methods is limited. ABMs are uniquely suited for the assessment of health determinants at multiple levels of influence that may couple with social interaction to produce population health. ABMs allow for the exploration of feedback and reciprocity between exposures and outcomes in the etiology of complex diseases. They may also provide the opportunity for counterfactual simulation. However, appropriate implementation of ABMs requires a balance between mechanistic rigor and model parsimony, and the precision of output from complex models is limited. Social network and agent-based approaches are promising in social epidemiology, but continued development of each approach is needed.},
	urldate = {2020-04-29},
	journal = {Epidemiologic Perspectives \& Innovations : EP+I},
	author = {El-Sayed, Abdulrahman M and Scarborough, Peter and Seemann, Lars and Galea, Sandro},
	month = feb,
	year = {2012},
	pmid = {22296660},
	pmcid = {PMC3395878},
	pages = {1},
	file = {PubMed Central Full Text PDF:C\:\\Users\\duane\\Zotero\\storage\\K3S3HMMP\\El-Sayed et al. - 2012 - Social network analysis and agent-based modeling i.pdf:application/pdf}
}

@article{luke_systems_2012,
	title = {Systems {Science} {Methods} in {Public} {Health}},
	volume = {33},
	issn = {0163-7525},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3644212/},
	doi = {10.1146/annurev-publhealth-031210-101222},
	abstract = {Complex systems abound in public health. Complex systems are made up of heterogeneous elements that interact with one another, have emergent properties that are not explained by understanding the individual elements of the system, persist over time and adapt to changing circumstances. Public health is starting to use results from systems science studies to shape practice and policy, for example in preparing for global pandemics. However, systems science study designs and analytic methods remain underutilized and are not widely featured in public health curricula or training. In this review we present an argument for the utility of systems science methods in public health, introduce three important systems science methods (system dynamics, network analysis, and agent-based modeling), and provide three case studies where these methods have been used to answer important public health science questions in the areas of infectious disease, tobacco control, and obesity.},
	urldate = {2020-04-29},
	journal = {Annual review of public health},
	author = {Luke, Douglas A. and Stamatakis, Katherine A.},
	month = apr,
	year = {2012},
