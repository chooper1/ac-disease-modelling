	month = may,
	year = {2016},
	pmid = {27089550},
	pmcid = {PMC4861521},
	pages = {794--801},
	file = {PubMed Central Full Text PDF:C\:\\Users\\duane\\Zotero\\storage\\FGLREFKB\\Hastings et al. - 2016 - Outbreak of Middle East Respiratory Syndrome at Te.pdf:application/pdf}
