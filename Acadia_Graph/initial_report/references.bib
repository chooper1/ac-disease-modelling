	keywords = {Disease Outbreaks, Humans, Adolescent, Adult, Antiviral Agents, Asia, Southeastern, Child, Child, Preschool, Immunization Programs, Infant, Influenza A virus, Influenza Vaccines, Influenza, Human, Models, Statistical, Quarantine, Stochastic Processes, Thailand, Time Factors, Vaccination},
	pages = {1083--1087},
	file = {Submitted Version:C\:\\Users\\duane\\Zotero\\storage\\CLSSEQSI\\Longini et al. - 2005 - Containing pandemic influenza at the source.pdf:application/pdf}
